{
  "name": "rightmove-address-resolver",
  "version": "1.0.0",
  "description": "Multi-step pipeline to resolve full UK addresses from Rightmove listings",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/api-server.js",
    "dev": "ts-node src/index.ts",
    "dev:api": "ts-node src/api-server.ts",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docker:build": "docker build -t address-resolver .",
    "docker:run": "docker run -p 3000:3000 address-resolver",
    "docker:compose": "docker-compose up --build"
  },
  "keywords": [
    "rightmove",
    "address",
    "resolver",
    "uk",
    "property"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "node-fetch": "^3.3.2",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "playwright": "^1.40.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
